extends layout

block resource
  link(href="css/style.css" rel="stylesheet" type="text/css")

block content
  .parallax
    .container
      .page-header
        h2= name
      each val in posts
        .panel.panel-default
          .panel-heading
            a(href="http://www.google.com"): h2 val.title
          .panel-body
            h3= val.sub
            
  script(type="text/javascript" src="http://code.jquery.com/jquery-2.1.3.min.js")
  script(type="text/javascript" src="../SubmitPost/js/main.js")
